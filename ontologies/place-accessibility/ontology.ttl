@prefix : <https://github.com/AKSW/leds-asp-f-ontologies/raw/master/ontologies/place-accessibility/ontology.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://github.com/AKSW/leds-asp-f-ontologies/raw/master/ontologies/place-accessibility/ontology.ttl#>
      a owl:Ontology ;
      dcterms:license <http://creativecommons.org/licenses/by-sa/4.0/> ;
      rdfs:comment "Ontologie zur Modellierung des Grads der Barrierefreiheit von Gebäuden."@de , "Ontology describing accessibility facts of places. It is based on https://github.com/AKSW/leds-asp-f-ontologies/blob/master/ontologies/place/ontology.ttl."@en .

:numberOfSlotsDisabledParkingLotInFrontOfPlace
      a owl:DatatypeProperty ;
      rdfs:label "Anzahl der Behindertenparkplätze vor der Einrichtung" .

:numberOfSlotsDisabledParkingLotOnSite
      a owl:DatatypeProperty ;
      rdfs:label "Anzahl der Behindertenparkplätze auf hauseigenem Parkplatz" .

:localSupport
      a owl:DatatypeProperty ;
      rdfs:label "Beschreibung der Hilfestellungen vor Ort" .

:supportForVisuallyImpairedAvailable
      a owl:DatatypeProperty ;
      rdfs:label "Besondere Hilfestellungen für Menschen mit Sehbehinderung vorhanden" .

:supportForHearingImpairedAvailable
      a owl:DatatypeProperty ;
      rdfs:label "Besondere Hilfestellungen für Menschen mit Hörbehinderung vorhanden" .

:altEntranceForWheelchairUserExisting
      a owl:DatatypeProperty ;
      rdfs:label "Alternativer Eingang für Rollstuhlfahrer vorhanden" .

:entranceSuitableForWheelchairs
      a owl:DatatypeProperty ;
      rdfs:label "Eingangsbereich rollstuhlgerecht" .

:locationAltEntranceForWheelchairUser
      a owl:DatatypeProperty ;
      rdfs:label "Ort des alternativen Eingangs für Rollstuhlfahrer" .

:elevatorSuitableForWheelchairs
      a owl:DatatypeProperty ;
      rdfs:label "Personenaufzug rollstuhlgerecht" .

:numberOfStepsTillElevator
      a owl:DatatypeProperty ;
      rdfs:label "Anzahl der Stufen bis Aufzug in der Einrichtung" .

wa:entranceDoorWidth
      a owl:DatatypeProperty ;
      rdfs:label "Kleinste Türbreite bis zum Erreichen der Einrichtung in cm" .

wa:elevatorDoorWidth
      a owl:DatatypeProperty ;
      rdfs:label "Aufzug Kabinen-Türbreite cm" .

wa:elevatorSpaceWidth
      a owl:DatatypeProperty ;
      rdfs:label "Aufzug Breite Innenkabine cm" .

wa:elevatorSpaceDepth
      a owl:DatatypeProperty ;
      rdfs:label "Aufzug Tiefe Innenkabine cm" .

:toiletSuitableForWheelchairs
      a owl:DatatypeProperty ;
      rdfs:label "Toilette rollstuhlgerecht" .

:markedWithPictogramAsDisabledToilet
      a owl:DatatypeProperty ;
      rdfs:label "Toilette mit Piktogramm als Behindertentoilette gekennzeichnet" .
